{
  "name": "Amazon Connect Extension",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "",
  "browser_action": {
    "default_title": "Fetch Contact Id"
  },
  "permissions": [
    "contentSettings"
  ],
  "web_accessible_resources": ["script.js", "contentScript.js"],
  "content_scripts": [
    {
      "matches": [
        "*://*.awsapps.com/*"
      ],
      "js": [
        "contentScript.js"
      ],
      "run_at": "document_end"
    }
  ]
}