{
  "name": "Amazon Connect Extension",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "",
  "browser_action": {
    "default_title": "Fetch Contact Id"
  },
  "permissions": [
    "contentSettings",
    "activeTab"
  ],
  "background" : {
    "scripts" : ["backgroundScript.js"]
  },  
  "web_accessible_resources": ["script.js", "contentScript.js", "testScript"],
  "content_scripts": [
    {
      "matches": [
        "*://*.awsapps.com/connect/ccp*"
      ],
      "js": [
        "contentScript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
